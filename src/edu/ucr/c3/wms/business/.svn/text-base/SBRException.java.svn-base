package edu.ucr.c3.wms.business;

import edu.ucr.c3.wms.servicelayer.ServiceException;

/**
 * @author ryanrich
 *
 */
public class SBRException extends ServiceException {

	/**
     * 
     */
    private static final long serialVersionUID = 1512129497058916144L;

    /**
	 * @param arg0
	 */
	public SBRException(String arg0) {
		super(arg0);
	}


	/**
	 * 
	 */
	public SBRException() {
		super();
	}
	
	/**
	 * @param message
	 * @param cause
	 */
	public SBRException(String message, Throwable cause) {
		super(message, cause);
	}
	
	/**
	 * @param cause
	 */
	public SBRException(Throwable cause) {
		super(cause);
	}
}
