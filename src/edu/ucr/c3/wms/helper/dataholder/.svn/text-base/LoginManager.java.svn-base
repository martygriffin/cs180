package edu.ucr.c3.wms.helper.dataholder;

import java.util.HashMap;
import java.util.Map;

import com.hibernate.User;

import edu.ucr.c3.phr.domain.Login;
import edu.ucr.c3.wms.domain.Admin;

public class LoginManager {
	

	private Admin admin;
	private User login;
	private Map<String,Boolean> loginPaths = new HashMap<String,Boolean>();
	
	public Map<String,Boolean> getLoginPaths() {
		return loginPaths;
	}
	public void setLoginPaths(Map<String,Boolean> loginPaths) {
		this.loginPaths = loginPaths;
	}
	
	//getters and setters
	

	public Admin getAdmin() {
		return admin;
	}
	public void setAdmin(Admin admin) {
		this.admin = admin;
	}
	public User getLogin() {
		return login;
	}
	public void setLogin(User login) {
		this.login = login;
	}

}
